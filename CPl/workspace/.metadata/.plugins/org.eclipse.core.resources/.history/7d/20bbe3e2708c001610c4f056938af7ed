#include "table.h"
#include <algorithm>
#include <iterator>
#include <numeric>
#include <vector>
#include <string>

void multab(std::ostream &out){
	// Create & fill vector with values from 1 to 20
    std::vector<int> v(20);
	iota(begin(v), end(v), 1);
	using ot = std::ostream_iterator<int>;
	copy(begin(v),end(v), ot{out, ", "});
	transforms(begin(v),end(v), begin(v), [&out](int x){
		out << x;
	})
}